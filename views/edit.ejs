<form style="width: 600px;margin-left: 500px; " method="post" enctype="multipart/form-data">
    <!-- Name input -->
    <div class="form-outline mb-4">
        <input type="text" id="form4Example1" class="form-control" name="name" value="<%=product.name%>" />
        <label class="form-label" for="form4Example1">Name</label>
    </div>
    <br>

    <!-- Email input -->
    <div class="form-outline mb-4">
        <input type="number" id="form4Example2" class="form-control" name="price" value="<%=product.price%>" />
        <label class="form-label" for="form4Example2">Price</label>
    </div>
    <br>
    <div class="form-outline mb-4">
        <input type="number" id="form4Example2" class="form-control" name="amount" value="<%=product.amount%>"/>
        <label class="form-label" for="form4Example2">Amount</label>
    </div>
    <br>
    <!-- Message input -->
    <div class="form-outline mb-4">
        <!--        <textarea  class="form-control" id="form4Example3" rows="4" name="description" ></textarea >-->
        <!--        <label class="form-label" for="form4Example3">description</label>-->
        <input type="text" id="form4Example2" class="form-control" name="description" value="<%=product.description%>"/>
        <label class="form-label" for="form4Example2">description</label>
    </div>
    <br>
    <div class="form-outline mb-4">
        <input type="file" id="form4Example2" class="form-control" name="image"/>
        <label class="form-label" for="form4Example2">image</label>
    </div>
    <br>
    <select name="idCategory"  style="margin-bottom: 50px;height: 20px" class="browser-default custom-select">
        <option selected>Open this select category</option>
        <% for (let category of categorys){ %>

        <option value="<%=category.idCategory%>"><%=category.name%></option>

        <%}%>
    </select>
    <!-- Checkbox -->


    <!-- Submit button -->
    <button type="submit" class="btn btn-primary btn-block mb-4">Send</button>
</form>

